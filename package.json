{
  "name": "vscode-module-templates",
  "version": "0.1.0",
  "description": "A VSCode extension for creating file/folder templates for new modules",
  "scripts": {
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asbjornh/vscode-module-templates.git"
  },
  "keywords": [
    "module",
    "template",
    "react",
    "vue",
    "angular"
  ],
  "author": "Asbj√∏rn Hegdahl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asbjornh/vscode-module-templates/issues"
  },
  "homepage": "https://github.com/asbjornh/vscode-module-templates#readme",
  "engines": {
    "vscode": "^1.41.1"
  },
  "main": "dist/source/extension.js",
  "activationEvents": [
    "onCommand:extension.newModuleFromTemplate"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.newModuleFromTemplate",
        "title": "New From Template"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.newModuleFromTemplate",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Module Templates",
      "properties": {
        "module-templates.templates": {
          "type": "array",
          "default": [],
          "description": "A list of templates. See readme for how to create a template."
        }
      }
    }
  },
  "dependencies": {
    "change-case": "^4.1.1",
    "fs-extra": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^13.1.6",
    "typescript": "^3.7.4",
    "vscode": "^1.1.36"
  }
}
